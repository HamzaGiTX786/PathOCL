    LastUpdatedBy: String
    LastUpdateDate: Date
end

class Departments
attributes
    Id: Real
    Name: String
    Location: String
    CreatedBy: String
    CreationDate: Date
    LastUpdatedBy: String
    LastUpdateDate: Date
end

-- associations

association SP between
    Suppliers[0..1] role suppliers
    Products[*] role products
end

association CO between
    Customers[0..1] role customers
    Orders[*] role orders
end

association CP between
    Customers[0..1] role customers
    Projects[*] role projects
end

association POL between
    Products[0..1] role products
    OrderLines[*] role orderlines
end

association OOL between
    Orders[0..1] role orders
    OrderLines[*] role orderlines
end

association PPA between
    Projects[0..1] role projects
    ProjectAssignments[*] role projectassignments
end

association EPA between
    Employees[0..1] role employees
    ProjectAssignments[*] role projectassignments
end

association EEJ between 
    Employees[1] role employees
    EmployeesJn[*] role employeejn
end

association EE between
    Employees[*] role employees
    Employees[0..1] role empidemployees
end

association ED between
    Employees[*] role employees
    Departments[0..1] role departments
end

