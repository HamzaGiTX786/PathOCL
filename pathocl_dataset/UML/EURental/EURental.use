-- associations

association CC between
    Car[1..*] role car
    CarGroup[1] role carGroup
end

association CR between
    Car[0..1] role car
    Rental[*] role rentalAgreement
end

association CGCG between
    CarGroup[0..1] role low
    CarGroup[0..1] role high
end

association CGB between
    CarGroup[1..*] role carGroup
    Branch[1..*] role branch
end

association BR between
    Branch[1] role pickUpBranch
    Rental[*] role pickUpRental
end

association BR2 between
    Branch[1] role dropOffBranch 
    Rental[*] role dropOffRental
end

association RC between
    Rental[1..*] role rental
    Customer[1..*] role driver
end

