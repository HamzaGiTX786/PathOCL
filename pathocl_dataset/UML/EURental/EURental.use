-- model name

model EURental

-- classes

class Date
attributes
    date: String
end

class Car
attributes
    regNum: String
end

class CarGroup
attributes
    category: String
end

class Branch
attributes
    address: String
end

class Quote
attributes
    value: Integer
end

class Rental
attributes
    id: String
    price: Real
    state: String
    startingDate: Date
    endingDate: Date
end

class Customer
attributes
    id: String
    name: String
    birthday: Date
    licenseExpDate: Date
    premium: Boolean
    discount: Integer
end

